{
    "name": "logiksuite/build",
    "description": "Logiksuite Build package.",
    "license": "MIT",
    "homepage": "https://logiksuite.com",
    "support": {
        "issues": "https://github.com/logiksuite/build/issues",
        "source": "https://github.com/logiksuite/build"
    },
    "authors": [
        {
            "name": "Benoit Gauthier",
            "email": "bgauthier@globalia.ca"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "fruitcake/laravel-cors": "^2.0",        
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.65",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "google/cloud-translate": "dev-master"    
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "google/cloud-translate": "dev-master",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "tabler/tabler" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "LogikSuite\\Build\\": "src/"            
        }
    },         
    "extra": {      
        "laravel": {
            "providers": [
                "LogikSuite\\Build\\LogikBuildServiceProvider"
            ]
        }
    },    
    "config": {
        "sort-packages": true
    },    
    "minimum-stability": "dev"
}
